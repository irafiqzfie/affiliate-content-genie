# Copy this file to .env.local and fill in real secrets. Do NOT commit real secrets to the repository.
# Example placeholders for required environment variables used by the app.

API_KEY=your_api_key_here
# For local dev using SQLite (example):
DATABASE_URL="file:./dev.db"

# OAuth / NextAuth (replace with your provider credentials and secure secret)
FACEBOOK_CLIENT_ID=your_facebook_client_id
FACEBOOK_CLIENT_SECRET=your_facebook_client_secret
THREADS_APP_ID=your_threads_app_id
THREADS_APP_SECRET=your_threads_app_secret
GITHUB_ID=your_github_client_id
GITHUB_SECRET=your_github_client_secret

# NextAuth configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here

# Add other provider or API vars below as needed
